{
  "app": {
    "name": "game-messaging-service",
    "service": "Game Messaging Service",
    "displayBanner": true,
    "version": "1"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "game-messaging-service",
    "address": "host.docker.internal",
    "port": "7002",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "game-messaging-service",
    "requestRetries": 3
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "game-messaging-service",
    "seed": false
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "game-messaging-service",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "elk": {
    "enabled": false,
    "url": "http://localhost:9200",
    "indexFormat": "game-messaging-service-{0:yyyy.MM.dd}",
    "basicAuthEnabled": false,
    "username": "user",
    "password": "secret"
  },
  "rabbitMq": {
    "conventionsCasing": "snakeCase",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false,
      "template": "game-messaging-service/{{exchange}}.{{message}}"
    }
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "messaging:"
  },
  "restEase": {
    "loadBalancer": "",
    "services": []
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": false,
    "url": "http://localhost:5341",
    "token": "secret"
  },
  "signalr": {
    "backplane": "redis",
    "hub": "messaging"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Messaging Service",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "key": "game-messaging-service/settings",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  }
}